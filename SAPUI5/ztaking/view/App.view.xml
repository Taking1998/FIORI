<mvc:View
  controllerName="ztaking.controller.App"
  xmlns="sap.m"
  xmlns:layout="sap.ui.layout"
  xmlns:core="sap.ui.core"
  xmlns:mvc="sap.ui.core.mvc">
  <Shell>
    <App id="app">
      <pages>
        <Page id="app-page" title="{i18n>appPageTitle}" titleAlignment="Center">
            <GenericTile 
              class="sapUiTinyMargin" 
              header="测试NotFound"
              press="navTo( ${app>/notFound} )"
              frameType="OneByOne">
              <TileContent>
                <core:Icon src="sap-icon://error" size="4rem"/>
              </TileContent>
            </GenericTile>
            <GenericTile 
              class="sapUiTinyMargin" 
              header="关于路由跳转"
              press="navTo( ${app>/aboutRoute} )"
              frameType="OneByOne">
              <TileContent>
                <core:Icon src="sap-icon://forward" size="4rem"/>
              </TileContent>
            </GenericTile>
            <GenericTile 
              class="sapUiTinyMargin" 
              header="IconTabBar"
              press="navTo( ${app>/iconTabBar} )"
              frameType="OneByOne">
              <TileContent>
                <core:Icon src="sap-icon://icon" size="4rem"/>
              </TileContent>
            </GenericTile>
            <GenericTile 
              class="sapUiTinyMargin" 
              header="employeeList"
              press="navTo( ${app>/employeeList} )"
              frameType="OneByOne">
              <TileContent>
                <core:Icon src="sap-icon://user" size="4rem"/>
              </TileContent>
            </GenericTile>
            <GenericTile 
              class="sapUiTinyMargin" 
              header="masterDetail"
              press="navTo( ${app>/masterDetail} )"
              frameType="OneByOne">
              <TileContent>
                <core:Icon src="sap-icon://user" size="4rem"/>
              </TileContent>
            </GenericTile>

          
        </Page>
      </pages>
    </App>
  </Shell>
</mvc:View>
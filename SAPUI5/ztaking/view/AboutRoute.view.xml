<mvc:View
    controllerName="ztaking.controller.AboutRoute"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <Page 
        id="oDataPage"
        title="{i18n>aboutRoute.title}"
        titleAlignment="Center"
        showNavButton="true" 
        navButtonPress="onNavBack">
        <Button text="{i18n>routeButtontext1}" class="sapUiSmallMarginBegin" press="onChangeUrl"/>
        <Button text="{i18n>routeButtontext2}" class="sapUiSmallMarginBegin" press="onNotChangeUrl"/>
    </Page>
</mvc:View>
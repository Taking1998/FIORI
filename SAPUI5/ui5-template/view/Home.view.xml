<mvc:View
   controllerName="sap.taking.controller.Home"
   xmlns="sap.m"
   xmlns:core="sap.ui.core"
   xmlns:mvc="sap.ui.core.mvc">
   <Page title="{i18n>homePageTitle}" titleAlignment="Center" class="homePage sapUiResponsiveContentPadding">
      <content>
         <Button id="displayNotFoundBtn" text="{i18n>displayNotFound}" press="onDisplayNotFound" class="sapUiTinyMarginEnd"/>
         <Button id="openDialogBtn" text="{i18n>openDialog}" press="onOpenDialog" class="sapUiTinyMarginEnd"/>
      </content>
   </Page>
</mvc:View>